	"tokenColors": [
		{
			"name": "Comment",
			"scope": [
				"comment",
				"punctuation.definition.comment"
			],
			"settings": {
				"fontStyle": "italic",
				"foreground": "#727796"
			}
		},
		{
			"name": "Variables",
			"scope": [
				"variable",
				"string constant.other.placeholder",
			],
			"settings": {
				"foreground": "#e3687c"
			}
		},
    {
      "name": "Variables",
      "scope": [
        "source.python",
            "meta.function-call.arguments.python",
      ],
